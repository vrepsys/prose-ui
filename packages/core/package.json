{
  "name": "@prose-ui/core",
  "version": "2.0.1",
  "license": "MIT",
  "homepage": "https://prose-ui.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vrepsys/prose-ui.git"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./metadata": "./dist/metadata.js",
    "./remark-code-block": {
      "types": "./dist/remark/remark-code-block.d.ts",
      "default": "./dist/remark/remark-code-block.js"
    },
    "./remark-image": {
      "types": "./dist/remark/remark-image.d.ts",
      "default": "./dist/remark/remark-image.js"
    },
    "./remark-inline-math": {
      "types": "./dist/remark/remark-inline-math.d.ts",
      "default": "./dist/remark/remark-inline-math.js"
    },
    "./remark-block-math": {
      "types": "./dist/remark/remark-block-math.d.ts",
      "default": "./dist/remark/remark-block-math.js"
    },
    "./remark-link": {
      "types": "./dist/remark/remark-link.d.ts",
      "default": "./dist/remark/remark-link.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "build": "rm -rf dist && tsc",
    "build:watch": "nodemon -e json --watch package.json --exec 'npm run build && npm pack'",
    "watch": "rm -rf dist && tsc --watch",
    "test": "vitest --pool=threads --poolOptions.threads.singleThread",
    "test:ci": "vitest run --pool=threads --poolOptions.threads.singleThread",
    "test:image": "vitest run --pool=threads --poolOptions.threads.singleThread -t image",
    "bump:rc": "pnpm version prerelease --preid=rc --no-git-tag-version",
    "publish:rc": "pnpm publish --access public --tag next --no-git-checks",
    "bump:latest": "pnpm version patch --no-git-tag-version",
    "publish:latest": "pnpm publish --access public --tag latest --no-git-checks"
  },
  "devDependencies": {
    "@prose-ui/eslint-config": "workspace:*",
    "@prose-ui/tsconfig": "workspace:*",
    "@types/estree": "^1.0.5",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20.17.10",
    "@types/unist": "^3.0.2",
    "remark": "^15.0.1",
    "remark-mdx": "^3.0.1",
    "typescript": "^5.4.5",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1",
    "acorn": "^8.12.1",
    "sharp": "^0.33.5",
    "hast-util-to-string": "^3.0.0",
    "mdast-util-definitions": "^6.0.0",
    "mdast-util-directive": "^3.0.0",
    "mdast-util-from-markdown": "^2.0.1",
    "mdast-util-gfm-table": "^2.0.0",
    "mdast-util-mdx-expression": "^2.0.0",
    "mdast-util-mdx-jsx": "^3.1.2",
    "mdast-util-mdxjs-esm": "^2.0.1",
    "mdast-util-to-markdown": "^2.1.0",
    "mdast-util-to-string": "^4.0.0",
    "micromark-extension-gfm-table": "^2.0.0",
    "micromark-extension-mdx-expression": "^3.0.0",
    "micromark-extension-mdx-jsx": "^3.0.0",
    "katex": "^0.16.11",
    "remark-custom-heading-id": "^2.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "slash": "^5.1.0",
    "shiki": "^3.14.0",
    "unified": "^11.0.4",
    "unist-util-filter": "^5.0.1",
    "unist-util-remove-position": "^5.0.0",
    "unist-util-visit": "^5.0.0"
  }
}
