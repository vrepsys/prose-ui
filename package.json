{
  "name": "prose-ui",
  "private": true,
  "resolutions": {
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test:ci": "turbo test:ci",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "bump:rc": "pnpm -r run bump:rc",
    "publish:rc": "pnpm -r run publish:rc",
    "bump:latest": "pnpm -r run bump:latest",
    "publish:latest": "pnpm -r run publish:latest",
    "release:rc": "pnpm build && pnpm bump:rc && pnpm publish:rc",
    "release:latest": "pnpm build && pnpm bump:latest && pnpm publish:latest"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.1.3",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  }
}
