{
  "name": "prose-ui",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "bump:rc": "pnpm -r run bump:rc",
    "publish:rc": "pnpm -r run publish:rc",
    "bump:latest": "pnpm -r run bump:latest",
    "publish:latest": "pnpm -r run publish:latest"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.1.3",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@9.13.2",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  }
}