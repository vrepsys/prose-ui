{
  "name": "@prose-ui/style",
  "version": "2.0.4",
  "license": "MIT",
  "homepage": "https://prose-ui.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vrepsys/prose-ui.git"
  },
  "exports": {
    "./prose-ui.css": "./dist/prose-ui.css",
    "./katex.min.css": "./dist/katex.min.css",
    "./fonts/*": "./dist/fonts/*"
  },
  "files": [
    "dist/prose-ui.css",
    "dist/katex.min.css",
    "dist/fonts",
    "README.md"
  ],
  "scripts": {
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "build": "rm -rf dist && tsc && pnpm build:css && pnpm copy:katex",
    "build:css": "ts-node src/scripts/build-css.ts",
    "copy:katex": "node scripts/copy-katex-css.cjs",
    "watch": "npx nodemon --watch src --ext ts --exec \"tsc && pnpm build:css\"",
    "bump:rc": "pnpm version prerelease --preid=rc --no-git-tag-version",
    "publish:rc": "pnpm publish --access public --tag next --no-git-checks",
    "bump:latest": "pnpm version patch --no-git-tag-version",
    "publish:latest": "pnpm publish --access public --tag latest --no-git-checks"
  },
  "devDependencies": {
    "@prose-ui/eslint-config": "workspace:*",
    "@prose-ui/tsconfig": "workspace:*",
    "@radix-ui/colors": "^3.0.0",
    "@types/node": "^20.17.10",
    "@types/postcss-js": "^4.0.4",
    "eslint": "^9.3.0",
    "jest": "^29.7.0",
    "katex": "^0.16.25",
    "postcss": "^8",
    "postcss-js": "^4.0.1",
    "postcss-nested": "^7.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
